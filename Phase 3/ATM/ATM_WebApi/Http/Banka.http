Content-Type: application/json

{
  "Ime": "Nova Banka",
  "Email": "contact@novabanka.com",
  "Web_adresa": "www.novabanka.com",
  "Adresa_centrale": "Ulica 123",
  "Filijala": [],
  "BrojeviTelefona": [],
  "Racuni": []
}

